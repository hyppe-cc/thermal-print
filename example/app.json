{
  "expo": {
    "name": "thermal-printer-example",
    "slug": "thermal-printer-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.thermalprinter.example",
      "infoPlist": {
        "UISupportedExternalAccessoryProtocols": ["com.epson.escpos"],
        "NSBluetoothAlwaysUsageDescription": "This app needs Bluetooth access to connect to thermal printers",
        "NSBluetoothPeripheralUsageDescription": "This app needs Bluetooth access to connect to thermal printers"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "expo.modules.thermalprinter.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
